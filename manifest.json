{ 
  "name": "CryptoViewer",
  "version": "1.0",
  "manifest_version": 2,
  "description": "仮想通貨のレートを確認",
  "browser_action": {
    "default_icon": {
      "19": "icon19.png"
    },
    "default_popup": "index.html" 
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "https://api.zaif.jp/*",
    "https://api.binance.com/*"
  ]
}
 
